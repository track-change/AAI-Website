---
import { urlFor } from "../utils/image";
import type { ImageAsset} from "@sanity/types";

interface Prop {
  asset: ImageAsset;
  alt: string;
}

const {asset, alt } = Astro.props as Prop
---

<img src={asset ? urlFor(asset).url() : ""} alt={alt} />

<style>
    img {
        height: 500px;
    }

</style>